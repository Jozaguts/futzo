{
  "name": "nuxt-app",
  "private": false,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nuxt build --max-old-space-size=4096 --dotenv .env.prod",
    "build:test": "nuxt build --dotenv .env",
    "dev": "rm -rf /tmp/nitro && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:nuxt": "vitest --project nuxt",
    "test:e2e": "RUN_E2E=1 vitest --project nuxt-e2e",
    "test:unit": "vitest --project unit",
    "test": "vitest",
    "pw:install": "npx -y playwright install --with-deps || true",
    "test:pw": "PW_E2E_EMAIL=$PW_E2E_EMAIL PW_E2E_PASSWORD=$PW_E2E_PASSWORD NUXT_PUBLIC_URL_BACKEND=${NUXT_PUBLIC_URL_BACKEND:-http://app.futzo.test} NUXT_PUBLIC_BACKEND_PREFIX=${NUXT_PUBLIC_BACKEND_PREFIX:-api/v1} playwright test",
    "test:pw:headed": "PW_E2E_EMAIL=$PW_E2E_EMAIL PW_E2E_PASSWORD=$PW_E2E_PASSWORD NUXT_PUBLIC_URL_BACKEND=${NUXT_PUBLIC_URL_BACKEND:-http://app.futzo.test} NUXT_PUBLIC_BACKEND_PREFIX=${NUXT_PUBLIC_BACKEND_PREFIX:-api/v1} playwright test --headed",
    "test:pw:ui": "playwright test --ui"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@mdi/font": "^7.4.47",
    "@morev/vue-transitions": "^3.0.5",
    "@nuxt/image": "1.11.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^9.5.2",
    "@pinia/nuxt": "^0.11.2",
    "@unlok-co/nuxt-stripe": "^5.0.0",
    "@vee-validate/nuxt": "^4.15.1",
    "@vee-validate/yup": "^4.15.1",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@vueuse/core": "^13.6.0",
    "@vueuse/nuxt": "^13.6.0",
    "dayjs": "^1.11.13",
    "nuxt": "^4.0.3",
    "nuxt-auth-sanctum": "^1.1.2",
    "nuxt-svgo": "4.2.6",
    "nuxt-typed-router": "^4.0.2",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.5.0",
    "three-dots": "^0.3.2",
    "v-phone-input": "^5.1.0",
    "vue": "latest",
    "vue-router": "latest",
    "vue-sonner": "^2.0.2",
    "vue3-google-map": "^0.23.1",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vuetify-sonner": "^0.3.21",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.2.5",
