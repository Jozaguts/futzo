    "@vee-validate/nuxt": "^4.15.1",
    "@vee-validate/yup": "^4.15.1",
    "@vuepic/vue-datepicker": "^12.0.5",
    "@vueuse/core": "^14.2.0",
    "@vueuse/nuxt": "^14.2.0",
    "dayjs": "^1.11.19",
    "nuxt": "^4.3.0",
    "nuxt-auth-sanctum": "^2.1.2",
    "nuxt-gtag": "4.1.0",
    "nuxt-schema-org": "^5.0.10",
    "nuxt-svgo": "4.2.6",
    "nuxt-tour": "^0.0.40",
    "nuxt-umami": "3.2.1",
    "pinia": "^3.0.4",
